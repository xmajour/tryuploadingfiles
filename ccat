                    } else {
                        document.getElementById("form1:domainName").value = Username;
                    }
                } catch (e) {
                    console.log(e);
                    document.getElementById("form1:userNameId").disabled = true;
//                    document.getElementById("form1:userNameId2").disabled = true;
                    clientLoginFlag = "true";
                }
