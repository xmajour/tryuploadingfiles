<script type="text/javascript">

//////////////////////
    var isIE = document.all ? true : false;
    var isNS = document.layers ? true : false;
    var allowEnterForMessage = false;

    document.onkeydown = function (e) {

        if (window.event.keyCode == 13 && allowEnterForMessage == true) {
            allowEnterForMessage = false;
            Richfaces.hideModalPanel('errorPanel');
            return false;
        }
        if (window.event.keyCode == 27) {
            allowEnterForMessage = false;
            Richfaces.hideModalPanel('errorPanel');
        }
    }

    function resetRefreshFlag() {
        allowEnterForMessage = true;
    }
//////////////////////
    function check() {
        var message = "";
        var userName = document.getElementById("form1:userNameId").value;
        var password = document.getElementById("form1:passwordId").value;
        //document.getElementById("form1:messageId").innerText="";
        document.getElementById("form1:errorPanelMessageId").innerText = "";
        if (userName == "") {
            message = " User name cannot be empty\n";
        }
        if (password == "") {
            message += "Password cannot be empty\n";
        }
        if (message != "") {
            //document.getElementById("form1:messageId").innerText=message;
            document.getElementById("form1:errorPanelMessageId").innerText = message;
            Richfaces.showModalPanel('errorPanel');
            return false;
        }
        return true;
    }

    function login() {
        var loginSubmit = check();
        if (loginSubmit === true) {
            document.getElementById("form1:link2").click();
        }
    }
</script>


