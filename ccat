                try {
                    var clientName = null;
                    var clientLoginFlag = document.getElementById("form1:clientFlagID").value;
                        var wshell = new ActiveXObject("WScript.Network");
                        var MachineName = wshell.ComputerName;
                        var Domain = wshell.UserDomain;
                        var Username = wshell.UserName;
                        document.getElementById("form1:machineName").value = MachineName;
                        document.getElementById("form1:domainName").value = Domain;
                    if (clientLoginFlag == "true") {
                        if (!(Username == null || Username == "")) {
                            // name retreived succefully
                            document.getElementById("form1:userNameId").value = Username;
                            document.getElementById("form1:userNameId2").value = Username;
                        }
                        document.getElementById("form1:NTAccount").value = Username;
                        document.getElementById("form1:userNameId").disabled = true;
